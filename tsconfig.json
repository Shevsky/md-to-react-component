{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "removeComments": true,
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true
  }
}
